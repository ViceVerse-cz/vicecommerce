{
    "$schema": "https://turborepo.org/schema.json",
    "globalDependencies": [
      ".env"
    ],
    "globalEnv": [
      "NODE_ENV",
      "CI"
    ],
    "pipeline": {
      "clean": {
        "dependsOn": [
          "^clean"
        ]
      },
      "build": {
        "dependsOn": [
          "clean",
          "^build"
        ],
        "outputs": [
          "dist/**",
          ".next/**"
        ]
      },
      "test": {
        "outputs": []
      },
      "test:ci": {
        "outputs": []
      },
      "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false,
        "env": [
            "MINIO_ENDPOINT",
            "MINIO_BUCKET",
            "MINIO_ACCESS_KEY",
            "MINIO_SECRET_KEY",
            "MEILISEARCH_HOST",
            "MEILI_MASTER_KEY"
        ]
      },
      "seed": {
        "cache": false,
        "env": [
            "MINIO_ENDPOINT",
            "MINIO_BUCKET",
            "MINIO_ACCESS_KEY",
            "MINIO_SECRET_KEY",
            "MEILISEARCH_HOST",
            "MEILI_MASTER_KEY"
        ]
      },
      "start": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  }