{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    ".env"
  ],
  "globalEnv": [
    "NODE_ENV",
    "CI"
  ],
  "pipeline": {
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "build": {
      "dependsOn": [
        "clean",
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "test": {
      "outputs": []
    },
    "test:ci": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "env": [
        "S3_URL",
        "S3_BUCKET",
        "S3_REGION",
        "S3_ACCESS_KEY_ID",
        "S3_SECRET_ACCESS_KEY",
        "MEILISEARCH_HOST",
        "MEILI_MASTER_KEY",
        "NEXT_PUBLIC_INDEX_NAME",
        "NEXT_PUBLIC_VERCEL_URL",
        "NEXT_PUBLIC_MEDUSA_BACKEND_URL",
        "FEATURE_SEARCH_ENABLED",
        "DATABASE_URL",
        "CONTENTFUL_SPACE_ID",
        "CONTENTFUL_ACCESS_TOKEN",
        "CONTENTFUL_ENV"
      ]
    },
    "seed": {
      "cache": false,
      "env": [
        "S3_URL",
        "S3_BUCKET",
        "S3_REGION",
        "S3_ACCESS_KEY_ID",
        "S3_SECRET_ACCESS_KEY",
        "MEILISEARCH_HOST",
        "MEILI_MASTER_KEY",
        "NEXT_PUBLIC_INDEX_NAME",
        "NEXT_PUBLIC_VERCEL_URL",
        "NEXT_PUBLIC_MEDUSA_BACKEND_URL",
        "FEATURE_SEARCH_ENABLED",
        "DATABASE_URL",
        "CONTENTFUL_SPACE_ID",
        "CONTENTFUL_ACCESS_TOKEN",
        "CONTENTFUL_ENV"
      ]
    },
    "start": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}